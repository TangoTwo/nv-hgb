 CellChangeTimes->{{3.751027236110024*^9, 3.751027242680052*^9}, 
   3.751027934013421*^9},
 CellLabel->"Out[24]=",ExpressionUUID->"d1324a1a-156f-48ed-896c-bffb9baee8fb"]
}, Open  ]]
}, Open  ]],

