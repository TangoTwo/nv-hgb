  3.751024746208571*^9},ExpressionUUID->"f32515e6-ebec-44d5-80c0-\
366aa41ddb6c"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"eW", " ", "=", " ", 
  RowBox[{
   RowBox[{"eG", "*", 
    RowBox[{"1", "/", "2"}]}], "+", 
   RowBox[{"eU", "*", 
    RowBox[{"1", "/", "3"}]}], "+", 
